{
    "name": "confetticode/laravel-foundation",
    "description": "Override Laravel Foundation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Quynh Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.77",
        "symfony/dotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "ConfettiCode\\Laravel\\Foundation\\": "src/Foundation/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConfettiCode\\Laravel\\Foundation\\Providers\\FoundationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
